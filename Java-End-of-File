import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=0;
        do 
        {
            ++n;
            System.out.println(n+" "+sc.nextLine());
        }while(sc.hasNext());
    }
}

